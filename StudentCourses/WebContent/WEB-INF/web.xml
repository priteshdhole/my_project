<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name>StudentCourses</display-name>
  <welcome-file-list>
    <welcome-file>login.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>Login</servlet-name>
    <servlet-class>org.nlt.controller.LoginController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Login</servlet-name>
    <url-pattern>/login</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>CourseC</servlet-name>
    <servlet-class>org.nlt.controller.CourseController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>CourseC</servlet-name>
    <url-pattern>/course</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>StudentC</servlet-name>
    <servlet-class>org.nlt.controller.StudentController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>StudentC</servlet-name>
    <url-pattern>/student</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>TestC</servlet-name>
    <servlet-class>org.nlt.controller.TestController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>TestC</servlet-name>
    <url-pattern>/test</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>AjaxC</servlet-name>
    <servlet-class>org.nlt.controller.AjaxController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>AjaxC</servlet-name>
    <url-pattern>/ajax</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>ReceiptC</servlet-name>
    <servlet-class>org.nlt.controller.ReceiptController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ReceiptC</servlet-name>
    <url-pattern>/receipt</url-pattern>
  </servlet-mapping>
</web-app>